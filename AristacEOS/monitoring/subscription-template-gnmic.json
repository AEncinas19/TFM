{
    "address": "arista:6030",
    "username": "admin",
    "password": "ADMIN",
    "insecure": true,
    "log-file": "/tmp/gnmic-1.log",
    "subscriptions": {
        "sub-periodic": {
            "paths": [
                "/interfaces/interface[name=Ethernet1]/state/counters/in-octets",
                "/interfaces/interface[name=Ethernet2]/state/counters/in-octets",
                "/interfaces/interface[name=Management0]/state/counters/in-octets"
            ],
            "stream-mode": "sample",
            "sample-interval": "5s",
            "encoding": "JSON",
            "qos": 0
        },
        "sub-on-change": {
            "paths": [
                "/interfaces/interface[name=Ethernet1]/state/admin-status",
                "/interfaces/interface[name=Ethernet2]/state/admin-status"
            ],
            "stream-mode": "on-change",
            "encoding": "JSON"
        }
    },
    "outputs": {
        "output": {
            "type": "kafka",
            "address": "kafka-service:9092",
            "topic": "gnmic-telemetry-1",
            "max-retry": 2,
            "timeout": "5s",
            "recovery-wait-time": "10s",
            "format": "event",
            "num-workers": 1,
            "debug": false
        }
    }
}
